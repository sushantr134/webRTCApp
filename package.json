{
  "name": "webrtc",
  "productName": "webrtc",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "build": "electron-packager . app --platform win32 --arch x64 --out dist/",
    "lint": "echo \"No linting configured\""
  },
  "bin": {
    "electron-webrtc": "./launch.js"
  },
  "keywords": [],
  "author": {
    "name": "Sushant Singh",
    "email": "sushantr134@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "webrtc"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "browserify": "^16.5.0",
    "domify": "^1.4.0",
    "ejs": "^3.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "simple-peer": "^9.6.2",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.47",
    "@electron-forge/maker-deb": "6.0.0-beta.47",
    "@electron-forge/maker-rpm": "6.0.0-beta.47",
    "@electron-forge/maker-squirrel": "6.0.0-beta.47",
    "@electron-forge/maker-zip": "6.0.0-beta.47",
    "electron": "7.2.4",
    "electron-packager": "^14.1.1",
    "electron-prebuilt": "^1.4.13"
  }
}
